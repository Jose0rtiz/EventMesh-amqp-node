{
  "name": "@sap/xb-msg",
  "version": "0.9.12",
  "description": "XB Messaging",
  "keywords": [
    "xb",
    "messaging"
  ],
  "license": "SEE LICENSE IN developer-license-3.1.txt",
  "main": "./index.js",
  "engines": {
    "node": ">=6.9.1"
  },
  "dependencies": {
    "@sap/xb-msg-amqp-v091": ">=0.9.12",
    "@sap/xb-msg-amqp-v100": ">=0.9.48",
    "@sap/xb-msg-mqtt-v311": ">=0.9.14"
  },
  "devDependencies": {
    "filter-node-package": "=2.2.0",
    "jsdoc": "=3.6.4",
    "mocha": "=8.0.1",
    "nyc": "=15.1.0",
    "sinon": "=9.0.2"
  },
  "files": [
    "examples",
    "lib",
    "index.js",
    "package.json",
    "README.md",
    "CHANGELOG.m",
    "npm-shrinkwrap.json"
  ],
  "scripts": {
    "test": "npm run all-tests",
    "all-tests": "node ./node_modules/mocha/bin/_mocha test/**/test*.js --colors -csdlJson",
    "cover": "npm run cover-unit-tests",
    "cover-unit-tests": "nyc --reporter=lcov --reporter=text-summary --report-dir ./_gen/coverage --temp-dir ./_gen/coverage/raw-output npm run test",
    "doc": "jsdoc -d ./_gen/doc -R README.md -r ./lib",
    "prepareRelease": "clean-packages && npm prune --production"
  }
}